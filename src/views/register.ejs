<%- include('inc/head.ejs')%>

<header id="header">
    <div class="hd-tit">
        <a href="/">
            <img src="/public/images/imagesK/backBtn.svg" alt="뒤로가기 버튼" class="back-ico">
        </a>
    </div>
</header>

<div class="header-hello">
    <h1> Register </h2>
</div>

<div id="container">
    <!-- 입력창 버튼 -->
    <form id="signUpform" class="btn-wrap" method="post" action="/register" enctype="multipart/form-data" >
        <input class="btn-comn signup-btn btn-line" placeholder="Id : Up to 10 characters" name="user_id" maxlength="10" >
        <input class="btn-comn signup-btn btn-line" placeholder="Nick Name : Up to 8 characters" name="user_nick" maxlength="8">
        <input class="btn-comn signup-btn btn-line" placeholder="Password : Up to 10 characters" name="user_pw" maxlength="10">
        <button class="btn-comn login-btn">Register</button>
    </form>
    <!-- 로고 -->
    <img src="/public/images/imagesK/miniLogo.svg" alt="영어로고" class="logo-txt">
</div>

<script src="/public/js/register.js"></script>
<%- include('inc/tail.ejs')%>